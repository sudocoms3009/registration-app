<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.facultyhandbook">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".PreviousRulesActivity"></activity>
        <activity android:name=".CurriculaActivity" />
        <activity android:name=".CourseInfoActivity" />
        <activity android:name=".AppContentActivity" />
        <activity android:name=".GovernanceActivity" />
        <activity android:name=".FacultyLocationActivity" />
        <activity android:name=".CoursesMainActivity" />
        <activity android:name=".HumanitiesActivity" />
        <activity android:name=".HealthActivity" />
        <activity android:name=".EngineeringActivity" />
        <activity android:name=".CommerceActivity" />
        <activity android:name=".ScienceActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>